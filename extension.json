{
	"name": "ExternalGuidance",
	"version": "1.0.0",
	"author": [
		"Santhosh Thottingal",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ExternalGuidance",
	"descriptionmsg": "externalguidance-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"ExtensionMessagesFiles": {
		"ExternalGuidanceAliases": "ExternalGuidance.alias.php"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\ExternalGuidance\\Hooks": "includes/Hooks.php",
		"MediaWiki\\Extension\\ExternalGuidance\\SiteMapper": "includes/SiteMapper.php",
		"MediaWiki\\Extension\\ExternalGuidance\\SpecialExternalGuidance": "specials/SpecialExternalGuidance.php"
	},
	"SpecialPages": {
		"ExternalGuidance": "MediaWiki\\Extension\\ExternalGuidance\\SpecialExternalGuidance"
	},
	"ConfigRegistry": {
		"ExternalGuidance": "GlobalVarConfig::newInstance"
	},
	"config": {
		"ExternalGuidanceSimulate": {
			"value": false,
			"description": "Simulate that the page is in an external context like machine translation."
		},
		"ExternalGuidanceMTReferrers": {
			"value": [],
			"description": "Host names of machine translation services"
		},
		"ExternalGuidanceSiteTemplates": {
			"value": {
				"view": "//$1.wikipedia.org/wiki/$2",
				"action": "//$1.wikipedia.org/w/index.php?title=$2",
				"api": "//$1.wikipedia.org/w/api.php"
			},
			"description": "URL templates for sites corresponding to other languages"
		},
		"ExternalGuidanceDomainCodeMapping": {
			"value": {
				"be-tarask": "be-x-old",
				"bho": "bh",
				"crh-latn": "crh",
				"gsw": "als",
				"lzh": "zh-classical",
				"nan": "zh-min-nan",
				"nb": "no",
				"rup": "roa-rup",
				"sgs": "bat-smg",
				"vro": "fiu-vro",
				"yue": "zh-yue"
			},
			"description": "Language to domain name mapping"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\ExternalGuidance\\Hooks::addModules",
		"ResourceLoaderGetConfigVars": "MediaWiki\\Extension\\ExternalGuidance\\Hooks::addConfig"
	},
	"MessagesDirs": {
		"ExternalGuidance": [
			"i18n"
		]
	},
	"ResourceModules": {
		"mw.externalguidance.init": {
			"targets": [ "mobile", "desktop" ],
			"skinScripts": {
				"minerva": [ "mw.ExternalGuidance.init.js" ]
			},
			"dependencies": [
				"mediawiki.Uri"
			],
			"position": "top"
		},
		"mw.externalguidance": {
			"targets": [ "mobile", "desktop" ],
			"skinScripts": {
				"minerva": [ "mw.ExternalGuidance.js" ]
			},
			"skinStyles": {
				"minerva": [ "mw.ExternalGuidance.less"]
			},
			"messages": [
				"externalguidance-machine-translation-heading",
				"externalguidance-machine-translation-contribute",
				"externalguidance-machine-translation-page-exist",
				"externalguidance-machine-translation-page-missing"
			],
			"dependencies": [
				"mw.externalguidance.icons",
				"mw.externalguidance.sitemapper",
				"oojs-ui.styles.icons-editing-core"
			]
		},
		"mw.externalguidance.icons": {
			"position": "top",
			"class": "ResourceLoaderImageModule",
			"selector": ".mw-ui-icon-eg-{name}:before",
			"images": {
				"robot": "images/robot.svg",
				"user": "images/userAvatar.svg"
			}
		},
		"mw.externalguidance.simulate": {
			"targets": [ "mobile", "desktop" ],
			"skinScripts": {
				"minerva": [ "mw.ExternalGuidance.simulate.js" ]
			},
			"position": "top"
		},
		"mw.externalguidance.sitemapper": {
			"targets": [ "mobile", "desktop" ],
			"skinScripts": {
				"minerva": [ "mw.ExternalGuidance.sitemapper.js" ]
			},
			"dependencies": [
				"mediawiki.ForeignApi"
			]
		},
		"mw.externalguidance.special": {
			"targets": [ "mobile", "desktop" ],
			"skinStyles": {
				"minerva": [ "mw.ExternalGuidance.specialpage.less"]
			},
			"dependencies": ["mw.externalguidance.icons"]
		},
		"mw.externalguidance.mt.info": {
			"targets": [ "mobile", "desktop" ],
			"dependencies": [
				"mobile.startup"
			],
			"scripts": [
				"mw.ExternalGuidance.mt.info/MTServiceInfo.js"
			],
			"styles":[
				"mw.ExternalGuidance.mt.info/MTServiceOverlay.less"
			],
			"templates": {
				"MTServiceOverlay.hogan": "mw.ExternalGuidance.mt.info/MTServiceOverlay.hogan"
			},
			"messages": [
				"externalguidance-machine-translation-provider-info-title",
				"externalguidance-machine-translation-provider-info",
				"externalguidance-machine-translation-provider-terms",
				"externalguidance-machine-translation-access-source-title",
				"externalguidance-machine-translation-access-source-page-missing",
				"externalguidance-machine-translation-access-source-page-exist",
				"externalguidance-machine-translation-contribute-link"
			]
		},
		"mw.externalguidance.createpage": {
			"targets": [ "mobile", "desktop" ],
			"dependencies": [
				"mobile.startup",
				"mw.externalguidance.sitemapper"
			],
			"scripts": [
				"mw.ExternalGuidance.createpage.js"
			],
			"styles":[
				"mw.ExternalGuidance.createpage.less"
			],
			"messages": [
				"externalguidance-specialpage-createpage-title",
				"externalguidance-specialpage-createpage-title-label",
				"externalguidance-specialpage-createpage-button-label",
				"externalguidance-specialpage-createpage-button-label-edit",
				"externalguidance-specialpage-createpage-desc",
				"externalguidance-specialpage-createpage-page-exist"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "ExternalGuidance/modules"
	},
	"manifest_version": 2
}
